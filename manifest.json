{
    "manifest_version": 3,
    "name": "Bookmarker",
    "version": "1.0",
    "description": "Easy way to keep your bookmarks.",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png"
        },
        "default_popup": "structure.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "js": [
                "dist/logic.js"
            ],
            "css": [
                "dist/style.min.css"
            ]
        }
    ]
}